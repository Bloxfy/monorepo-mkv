model Wallet {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  ownerId   String    @db.ObjectId
  user      User      @relation(fields: [ownerId], references: [id])
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?

  @@map("wallets")
}