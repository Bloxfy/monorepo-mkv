{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*",
        "package.json",
        "tsconfig.json"
      ],
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".nuxt/**"
      ]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build", "^lint"]
    },
    "check-types": {
      "dependsOn": ["^build", "^check-types"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "db:push": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "type:generate": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    }
  }
}